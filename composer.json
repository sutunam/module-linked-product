{
    "name": "sutunam/module-linked-product",
    "description": "Sutunam Linked Product Module",
    "type": "magento2-module",
    "version": "1.0.4",
    "require": {
        "magento/framework": "*",
        "magento/module-related-product-graph-ql": "*",
        "magento/module-catalog-widget": "*",
        "sutunam/module-base": "*"
    },
    "license": [
        "GPL-3.0-or-later"
    ],
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "Sutunam\\LinkedProduct\\": "src"
        }
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true
        }
    }
}
